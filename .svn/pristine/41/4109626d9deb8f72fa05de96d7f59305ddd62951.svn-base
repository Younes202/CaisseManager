package appli.controller.domaine.stock.bean;

import java.math.BigDecimal;

public class PaiementAchatBean{
	private String modePaie;
	private BigDecimal mttTtc;
	private BigDecimal quantite;
	
	public String getModePaie() {
		return modePaie;
	}
	public void setModePaie(String modePaie) {
		this.modePaie = modePaie;
	}
	public BigDecimal getMttTtc() {
		return mttTtc;
	}
	public void setMttTtc(BigDecimal mttTtc) {
		this.mttTtc = mttTtc;
	}
	public BigDecimal getQuantite() {
		return quantite;
	}
	public void setQuantite(BigDecimal quantite) {
		this.quantite = quantite;
	}
}
