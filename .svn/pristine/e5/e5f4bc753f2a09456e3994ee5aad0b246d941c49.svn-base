package framework.controller.bean;

public class DialogButtonBean {
	private String label;
	private String action;
	private String params;
	
	/**
	 * 
	 */
	public DialogButtonBean(){}
	
	/**
	 * @param label
	 * @param action
	 */
	public DialogButtonBean(String label, String action){
		this.label = label;
		this.action = action;
	}
	public DialogButtonBean(String label, String action, String params){
		this.label = label;
		this.action = action;
		this.params = params;
	}	
	/**
	 * @return the action
	 */
	public String getAction() {
		return action;
	}
	/**
	 * @param action the action to set
	 */
	public void setAction(String action) {
		this.action = action;
	}
	/**
	 * @return the label
	 */
	public String getLabel() {
		return label;
	}
	/**
	 * @param label the label to set
	 */
	public void setLabel(String label) {
		this.label = label;
	}

	public String getParams() {
		return params;
	}

	public void setParams(String params) {
		this.params = params;
	}
}
