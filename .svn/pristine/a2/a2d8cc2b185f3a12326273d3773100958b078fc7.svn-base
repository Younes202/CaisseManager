<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<description>Complex component</description>
	<display-name>complex</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>cplx</short-name>
	<uri>http://www.customtaglib.com/complexe</uri>
	<!-- ******************************************** Table ***************************************************** -->
	<tag>
		<name>table</name>
		<tag-class>framework.component.complex.table.TableTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>dragable</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>initAction</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>align</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>classStyle</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Custom height (Integer) if default is not ok</description>
			<name>trHeight</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Default false. If true the vertical scroll is hidden</description>
			<name>autoHeight</name>
		</attribute>
		<attribute>
			<name>showTitleBanner</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>sortable</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>filtrable</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>checkable</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>exportable</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>showDataState</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>paginate</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>alternate</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>title</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Force show filter banner</description>
			<name>forceFilter</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>titleKey</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Type of transition. Supported values : simple-full-multiple. Default = simple  </description>
			<name>transitionType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- ******************************************** Th ***************************************************** -->
	<tag>
		<name>th</name>
		<tag-class>framework.component.complex.table.ThTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>classStyle</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>align</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>filterOnly</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>filtrable</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>sortable</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>field</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>fieldExport</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>valueKey</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>autocompleteAct</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Types can be used : empty - integer, long - decimal - string - boolean - date - dateTime</description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Types can be used : String[][], List or Set </description>
			<name>groupValues</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Field to use for filter data. Required if type Set or List was used</description>
			<name>groupKey</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Field to show in group select. Required if type Set or List was used. You can use code;'-';libelle for concatenation</description>
			<name>groupLabel</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>No format : default true</description>
			<name>formatable</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>		
		<attribute>
			<description>Activate left join (true or false). Default false</description>
			<name>leftJoin</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>				
	</tag>
	<!-- ******************************************** Tr ***************************************************** -->
	<tag>
		<name>tr</name>
		<tag-class>framework.component.complex.table.TrTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>workId</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>checkable</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>classStyle</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>align</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<!-- ******************************************** Td ***************************************************** -->
	<tag>
		<name>td</name>
		<tag-class>framework.component.complex.table.TdTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>classStyle</name>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>align</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- ******************************************** Header ***************************************************** -->
	<tag>
		<name>header</name>
		<tag-class>framework.component.complex.table.HeaderTag</tag-class>
		<body-content>JSP</body-content>
	</tag>
	<!-- ******************************************** Body ***************************************************** -->
	<tag>
		<name>body</name>
		<tag-class>framework.component.complex.table.BodyTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>Override current controller</description>
			<name>controller</name>
		</attribute>			
	</tag>
	<!-- ******************************************** Export ***************************************************** -->
	<tag>
		<name>export</name>
		<tag-class>framework.component.complex.table.export.ExportTag</tag-class>
		<body-content>JSP</body-content>
	</tag>
	<!-- ******************************************** Export th ***************************************************** -->
	<tag>
		<name>expth</name>
		<tag-class>framework.component.complex.table.export.ExportThTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>field</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>width</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>valueKey</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Types can be used : empty - long - decimal - string - boolean - date - dateTime</description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Types can be used : String[][]</description>
			<name>groupValues</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- ******************************************** Dynamic row ***************************************************** -->
</taglib>
