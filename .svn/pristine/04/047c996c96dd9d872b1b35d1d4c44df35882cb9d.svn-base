package appli.model.domaine.stock.service.impl;

import java.math.BigDecimal;

public class RepartitionBean{
	private String libelle;
	private String famille;
	private BigDecimal montant;
	private BigDecimal quantite;
//	private BigDecimal montantMenu;
//	private BigDecimal montantHorsMenu;
	private BigDecimal montantMargeCaissier;
//	private BigDecimal quantiteMenu;
//	private BigDecimal quantiteHorsMenu;
	private Long elementId;
	private int nbrCmd;
	
	public String getLibelle() {
		return libelle;
	}
	public Long getElementId() {
		return elementId;
	}
	public void setElementId(Long elementId) {
		this.elementId = elementId;
	}
	public void setLibelle(String libelle) {
		this.libelle = libelle;
	}
	public String getFamille() {
		return famille;
	}
	public void setFamille(String famille) {
		this.famille = famille;
	}
	public BigDecimal getMontant() {
		return montant;
	}
	public void setMontant(BigDecimal montant) {
		this.montant = montant;
	}
	public BigDecimal getQuantite() {
		return quantite;
	}
	public void setQuantite(BigDecimal quantite) {
		this.quantite = quantite;
	}
	public int getNbrCmd() {
		return nbrCmd;
	}
	public void setNbrCmd(int nbrCmd) {
		this.nbrCmd = nbrCmd;
	}
	public BigDecimal getMontantMargeCaissier() {
		return montantMargeCaissier;
	}
	public void setMontantMargeCaissier(BigDecimal montantMargeCaissier) {
		this.montantMargeCaissier = montantMargeCaissier;
	}
}