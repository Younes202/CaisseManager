   	public void setGroupElement(CaisseMouvementArticlePersistant groupElement) {
   		this.groupElement = groupElement;
   	}

   	public List<MenuCmdBean> getListGroupe() {
   		return listGroupe;
   	}

   	public void setListGroupe(List<MenuCmdBean> listGroupe) {
   		this.listGroupe = listGroupe;
   	}

   	public List<CaisseMouvementArticlePersistant> getListArticle() {
