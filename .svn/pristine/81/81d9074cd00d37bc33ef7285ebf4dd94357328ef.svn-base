package appli.controller.domaine.caisse.bean;

import java.util.List;

import appli.model.domaine.vente.persistant.CaisseMouvementArticlePersistant;

public class MenuCmdBean {
   	private Integer clientIdx;
   	private String menuIdx;
   	private String groupId;
   	private CaisseMouvementArticlePersistant groupElement;
   	private List<MenuCmdBean> listGroupe;
   	private List<CaisseMouvementArticlePersistant> listArticle;
   	
   	
   	public Integer getClientIdx() {
   		return clientIdx;
   	}

   	public void setClientIdx(Integer clientIdx) {
   		this.clientIdx = clientIdx;
   	}

   	public String getMenuIdx() {
   		return menuIdx;
   	}

   	public void setMenuIdx(String menuIdx) {
   		this.menuIdx = menuIdx;
   	}

   	public String getGroupId() {
