		*****************************************************
	-->
	<tag>
		<name>tooltip</name>
		<tag-class>framework.component.Tooltip</tag-class>
		<body-content>empty</body-content>
		<attribute>
		   <name>name</name>
		   <rtexprvalue>true</rtexprvalue>
		 </attribute>
		<attribute>
			<description>True = Tooltip is fixed</description>
			<name>sticky</name>
		</attribute>
		<attribute>
			<description>Force show tooltip</description>
			<name>forceShow</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>helpPath</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The width of the clueTip</description>
			<name>width</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The height of the clueTip</description>
			<name>height</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>close when clueTip is moused out</description>
			<name>mouseOutClose</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>if true, clueTip will track mouse movement</description>
			<name>tracking</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>close clueTip on a timed delay</description>
			<name>delayedClose</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>contentKey</name>
		</attribute>
		<attribute>
			<name>content</name>
		</attribute>
		<attribute>
			<name>title</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>titleKey</name>
		</attribute>
		<attribute>
			<description>super, sub, baseline, inherit, text-bottom, bottom, middle, text-top, top. Default value : middle</description>
			<name>imgVAlign</name>
		</attribute>
	</tag>


	<!--******************************************** Ajax*****************************************************-->
	<tag>
		<name>ajax</name>
		<tag-class>framework.component.Ajax</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>Event triggering ajax : blur, change, click, dblclick, focus, hover, keydown
				keypress, keyup, mousedown, mouseenter, mouseleave, mousemove, mouseout, mouseover, mouseup
			</description>
		   <name>event</name>
		   <required>true</required>
		   <rtexprvalue>true</rtexprvalue>
		 </attribute>
		<attribute>
			<description>Target to render. Ids separated by ;</description>
			<name>target</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Use if target is one input field. Action must write in response. Default false</description>
			<name>isInput</name>
		</attribute>
		<attribute>
			<description>Action to call. Default work_refresh</description>
			<name>action</name>
		</attribute>
		<attribute>
			<name>params</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Skip init action in controller : default true</description>
			<name>skipInit</name>
		</attribute>
		<attribute>
			<description>Skip post action in controller : default true</description>
			<name>skipPost</name>
		</attribute>
	</tag>
	
	<tag>
		<name>hidden</name>
		<tag-class>framework.component.text.Hidden</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>iskey</name>
		</attribute>	
		<attribute>
			<description>Default : false. if true then it functions like basic hidden without auto value</description>
			<name>basic</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>			
	</tag>	
	
</taglib>
