package appli.controller.domaine.stock.bean;

import appli.model.domaine.stock.persistant.EmplacementPersistant;
import framework.controller.bean.action.IViewBean;

public class EmplacementBean extends EmplacementPersistant implements IViewBean {
	private String[] articles_array;
	private String[] familles_array;
	private String[] articles_ex_array;
	private String[] familles_ex_array;

	public String[] getArticles_array() {
		return articles_array;
	}

	public void setArticles_array(String[] articles_array) {
		this.articles_array = articles_array;
	}

	public String[] getFamilles_array() {
		return familles_array;
	}

	public void setFamilles_array(String[] familles_array) {
		this.familles_array = familles_array;
	}

	public String[] getArticles_ex_array() {
		return articles_ex_array;
	}

	public void setArticles_ex_array(String[] articles_ex_array) {
		this.articles_ex_array = articles_ex_array;
	}

	public String[] getFamilles_ex_array() {
		return familles_ex_array;
	}

	public void setFamilles_ex_array(String[] familles_ex_array) {
		this.familles_ex_array = familles_ex_array;
	}
}
