package framework.component.action;

import framework.component.ComponentBase;

/**
 * @author 
 * 
 */
@SuppressWarnings("serial")
public abstract class CompActionBase extends ComponentBase {
	private String action;
	private String actionGroup;
	private String params;
	private String icon;
	private String workId;
	private String targetDiv;
	private String forceShow;
	private String onComplete;
	
	public void releaseAll() {
		this.action = null;
		this.actionGroup = null;
		this.params = null;
		this.icon = null;
		this.workId = null;
		this.targetDiv = null;
		this.forceShow = null;
		this.onComplete = null;
	}
	
	/**
	 * @return the action
	 */
	public String getAction() {
		return action;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}
	/**
	 * @param action the action to set
	 */
	public void setAction(String action) {
		this.action = action;
	}
	
	public String getActionGroup() {
		return actionGroup;
	}

	public void setActionGroup(String actionGroup) {
		this.actionGroup = actionGroup;
	}

	public String getParams() {
		return params;
	}

	public void setParams(String params) {
		this.params = params;
	}

	public String getWorkId() {
		return workId;
	}

	public void setWorkId(String workId) {
		this.workId = workId;
	}

	public String getTargetDiv() {
		return targetDiv;
	}

	public void setTargetDiv(String targetDiv) {
		this.targetDiv = targetDiv;
	}

	public String getForceShow() {
		return forceShow;
	}

	public void setForceShow(String forceShow) {
		this.forceShow = forceShow;
	}
	
	public String getOnComplete() {
		return onComplete;
	}

	public void setOnComplete(String onComplete) {
		this.onComplete = onComplete;
	}
}
